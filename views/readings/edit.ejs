<%- include('../partials/header') %>
<div class="readings-nav">
        <form method="POST" action="/readings/<%= reading._id %>">
              <label for="question">
                  <input name="question" value="<%= question %>">  </input>
              </label>
              <label for="name">
                  <input name="name" value="<%= cardName %>"> </input>
              </label>
              <label for="meaning">
                  <input name="meaning" value="<%= cardMean %>">        </input>
              </label>
              <label for="desc">
                  <input name="desc" value="<%= cardDesc %>">          </input>  
              </label>
              <button type="submit">Add to Diary</button>
            </form>
        </div>    
<%- include('../partials/footer') %>
